HA;EAWAE,OAXA,qBAWA;IACA;EACA;AAbA","names":["name","computed","methods","addToCart","product","removeFromCart","created"],"sourceRoot":"Code/01_Intro/src/components","sources":["Products.vue"],"sourcesContent":["<template>\n  <div>\n      <h2>Products</h2>\n      <table>\n        <thead>\n        <tr>\n          <th>Name</th>\n          <td>&nbsp;&nbsp;&nbsp;</td>\n          <th>Price</th>\n          <th>&nbsp;</th>\n        </tr>\n        </thead>\n        <tbody>\n        <tr v-for=\"(product, idx) in products\" :key=\"idx\">\n          <td>{{ product.name }}</td>\n          <td>&nbsp;&nbsp;&nbsp;</td>\n          <td>{{ product.price.toFixed(2) }}</td>\n          <td>&nbsp;&nbsp;&nbsp;</td>\n          <td>\n            <b-button variant=\"success\"  @click=\"addToCart(product)\">+</b-button>\n            <b-button variant=\"danger\" @click=\"removeFromCart(product)\">-</b-button>\n          </td>\n        </tr>\n        </tbody>\n      </table>\n  </div>\n</template>\n\n<script>\nimport { mapState } from 'vuex';\nexport default {\n  name: 'Products',\n  computed: mapState(['products']),\n  methods: {\n    addToCart(product) {\n      this.$store.commit('addToCart', product);\n    },\n    removeFromCart(product) {\n      this.$store.commit('removeFromCart', product);\n    }\n  },\n  created() {\n    this.$store.dispatch(\"loadProducts\")\n  }\n}\n</script>\n<style>\nth, td {\n  padding: 10px;\n}\n\n</style>\n\n"]}]}