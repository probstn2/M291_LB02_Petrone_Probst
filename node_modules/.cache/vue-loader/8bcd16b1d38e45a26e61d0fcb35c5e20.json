trong> ({{ totalItems }})\n      <br/><strong>Betrag</strong> {{ formatCurrency(totalCost) }}</div>\n    <div class=\"even-columns\" v-for=\"item in products\" :key=\"item\">\n      <div class=\"col\">{{ item.title }}</div>\n      <div class=\"col\"><a href=\"#\" @click=\"addToCart(item.prize)\">Zum Warenkorb hinzuf√ºgen + {{ formatCurrency(item.prize) }}</a></div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      totalItems: 0,\n      totalCost: 0,\n      products: [\n        {title:\"Ring Silber, OneSize\", currency: \"CHF\", prize: 8.00},\n        {title:\"Ring Gold, OneSize\", currency: \"CHF\", prize: 10.00},\n        {title:\"Rose Gold, OneSize\", currency: \"CHF\", prize: 3.00},\n        {title:\"Kette Gold, 40cm\", currency: \"CHF\", prize: 18.50},\n        {title:\"Kette Gold, 50cm\", currency: \"CHF\", prize: 22.00},\n        {title:\"Kette Gold, 60cm\", currency: \"CHF\", prize: 30.00},\n      ],\n    }\n  },\n  methods: {\n    addToCart(n) {\n      this.totalItems = this.totalItems + 1\n      this.totalCost = this.totalCost + n\n    },\n    formatCurrency(val) {\n      return `CHF ${val.toFixed(2)}`\n    },\n  },\n}\n</script>\n\n<style lang=\"scss\" scoped>\n#content {\n  padding-left: 20px;\n}\n\n.even-columns {\n  display: flex;\n  //gap: 0.1em;\n  padding: 0.2em;\n}\n\n.even-columns > * {\n  flex: 1;\n}\n\n\na {\n  display: inline-block;\n  background: rgb(235, 50, 50);\n  border-radius: 10px;\n  font-size: 10px;\n  color: white;\n  padding: 5px 10px;\n  text-decoration: none;\n}\n</style>\n"]}]}